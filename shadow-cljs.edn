;; shadow-cljs configuration
{:source-paths
 ["src"
  "src/test"]

 :dependencies
 [[reagent "1.1.0"]]

 :dev-http
 {8020 {:root "public"
        :proxy-url "http://localhost:3000" ;; API server
        }}

 :builds
 {:app
  {:target :browser
   :output-dir "public/js"
   :asset-path "/js"

   :modules
   {:main ; becomes public/js/main.js
    {:init-fn starter.browser/init}}}}}
